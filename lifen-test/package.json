{
  "name": "lifen-test",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Anaïs Limpalaër",
    "email": "anais.limpalaer@gmail.com",
    "url": "https://www.lifen.fr"
  },
  "build": {
    "appId": "com.lifen.document-uploader",
    "productName": "Lifen Document Uploader",
    "copyright": "Copyright © 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "chokidar": "3.0.2",
    "electron-is-dev": "^1.1.0",
    "eslint-utils": "^1.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.1.8",
    "react-modal": "^3.10.1",
    "react-scripts": "3.1.1",
    "react-spinners": "^0.6.1"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -mw"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@jest-runner/electron": "^2.0.2",
    "@rescripts/cli": "^0.0.11",
    "@rescripts/rescript-env": "^0.0.10",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "concurrently": "^4.1.2",
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "patch-package": "^6.1.4",
    "postinstall-postinstall": "^2.0.0",
    "react-test-renderer": "^16.9.0",
    "wait-on": "^3.3.0"
  }
}
